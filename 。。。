SET NAMES UTF8;
DROP DATABASE IF EXISTS fl;
CREATE DATABASE fl CHARSET=UTF8;
USE fl;

create table fl_admin(
	aid INT PRIMARY KEY AUTO_INCREMENT,
	aname VARCHAR(32),
	apwd VARCHAR(32)
);
INSERT INTO fl_admin VALUES
('1','admin','123456'),
('2','','');


create table fl_user(
	uid INT PRIMARY KEY AUTO_INCREMENT,
	uname VARCHAR(32),
	upwd VARCHAR(32)
);

INSERT INTO fl_user VALUES
('1','18628996429','123456'),
('2','18628996666','123456');

create table fl_product(
	pid INT PRIMARY KEY AUTO_INCREMENT,
	pname VARCHAR(32),
	ppic VARCHAR(64),
	pkind varchar(16),
	pcolor varchar(16),
	pprice1 float(10,2),
	pprice2 float(10,2),
	pduixiang varchar(16),
	pzhishu	int,
	pyongtu varchar(16),
	pmiaoshu varchar(64),
	phuacai varchar(32),
	phuayu varchar(32),
	pbaozhuang varchar(32),
	pishot int,
	pxiaoliang int
);
